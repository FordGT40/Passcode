<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/head_title_bar" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:focusableInTouchMode="true"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_15"
                    android:paddingBottom="@dimen/dp_15">

                    <TextView
                        android:id="@+id/tv_25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/palce_name_hint"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_place_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dp_5"
                        android:layout_toLeftOf="@+id/iv_more1"
                        android:layout_toRightOf="@+id/tv_25"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="@string/hint_select"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15" />

                    <ImageView
                        android:id="@+id/iv_more1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/enter" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_15"
                    android:paddingBottom="@dimen/dp_15">

                    <TextView
                        android:id="@+id/tv_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/card_type"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_card_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dp_5"
                        android:layout_toLeftOf="@+id/iv_more"
                        android:layout_toRightOf="@+id/tv_2"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="@string/hint_select"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15" />

                    <ImageView
                        android:id="@+id/iv_more"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/enter" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_15"
                    android:paddingBottom="@dimen/dp_15">

                    <TextView
                        android:id="@+id/tv_26"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/plate_num"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold" />
                    <!--                <TextView-->
                    <!--                    android:id="@+id/tv_261"-->
                    <!--                    android:layout_width="wrap_content"-->
                    <!--                    android:layout_height="wrap_content"-->
                    <!--                    android:text="é»‘"-->
                    <!--                    android:layout_marginRight="@dimen/dp_10"-->
                    <!--                    android:layout_toLeftOf="@+id/iv_more3"-->
                    <!--                    android:textColor="@color/textcolor"-->
                    <!--                    android:textSize="@dimen/sp_15"-->
                    <!--                    android:textStyle="bold" />-->
                    <!--                <ImageView-->
                    <!--                    android:id="@+id/iv_more3"-->
                    <!--                    android:layout_width="wrap_content"-->
                    <!--                    android:layout_height="wrap_content"-->
                    <!--                    android:layout_toLeftOf="@+id/et_plate_num"-->
                    <!--                    android:layout_centerVertical="true"-->
                    <!--                    android:src="@drawable/enter" />-->

                    <EditText
                        android:id="@+id/et_plate_num"
                        android:layout_width="@dimen/dp_80"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/dp_10"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="@string/hint37"
                        android:maxLength="8"
                        android:textColor="@color/textcolor"
                        android:textColorHint="@color/ysf_grey_999999"
                        android:textSize="@dimen/sp_15" />

                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_15"
                    android:paddingBottom="@dimen/dp_15">

                    <TextView
                        android:id="@+id/tv_33"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/reason"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/et_reason"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dp_5"
                        android:layout_toRightOf="@+id/tv_33"
                        android:background="@null"
                        android:gravity="right"
                        android:maxLength="50"
                        android:hint="@string/hint_reason"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15" />


                </RelativeLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/ysf_grey_e9e9e9"
                    android:paddingLeft="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_5"
                    android:paddingRight="@dimen/dp_16"
                    android:paddingBottom="@dimen/dp_5"
                    android:text="@string/referrer_info"
                    android:textColor="@color/color_666666"
                    android:textSize="@dimen/sp_13" />

                <RelativeLayout
                    android:id="@+id/rl_person_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_15"
                    android:paddingBottom="@dimen/dp_15">

                    <TextView
                        android:id="@+id/tv_22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/name"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold" />


                    <EditText
                        android:id="@+id/et_person_id_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_toLeftOf="@+id/iv_2"
                        android:layout_toRightOf="@+id/tv_22"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="@string/name_hint"
                        android:textColor="@color/textcolor"
                        android:textColorHint="@color/ysf_grey_999999"
                        android:textSize="@dimen/sp_15" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />

                <RelativeLayout
                    android:id="@+id/rl_person_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_15"
                    android:paddingBottom="@dimen/dp_15">

                    <TextView
                        android:id="@+id/tv33"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/person_phone"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold" />


                    <EditText
                        android:inputType="phone"
                        android:text="15045689372"
                        android:id="@+id/et_person_phone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_toLeftOf="@+id/iv_2"
                        android:layout_toRightOf="@+id/tv33"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="@string/phone_hint"
                        android:textColor="@color/textcolor"
                        android:textColorHint="@color/ysf_grey_999999"
                        android:textSize="@dimen/sp_15" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_15"
                    android:paddingBottom="@dimen/dp_15">

                    <TextView
                        android:id="@+id/tv323"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/person_dep"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold" />


                    <EditText
                        android:id="@+id/et_person_dep"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_toLeftOf="@+id/iv_2"
                        android:layout_toRightOf="@+id/tv323"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="@string/dep_hint"
                        android:textColor="@color/textcolor"
                        android:textColorHint="@color/ysf_grey_999999"
                        android:textSize="@dimen/sp_15" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:paddingTop="@dimen/dp_15"
                    android:paddingBottom="@dimen/dp_15">

                    <TextView
                        android:id="@+id/tv36"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/person_job"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_15"
                        android:textStyle="bold" />


                    <EditText
                        android:id="@+id/et_person_job"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_toLeftOf="@+id/iv_2"
                        android:layout_toRightOf="@+id/tv36"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="@string/job_hint"
                        android:textColor="@color/textcolor"
                        android:textColorHint="@color/ysf_grey_999999"
                        android:textSize="@dimen/sp_15" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />
                <TextView
                    android:id="@+id/tv38"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/car_photo"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginTop="@dimen/dp_16"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/sp_15"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv381"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/car_photo_hint"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textColor="@color/ysf_grey_999999"
                    android:textSize="@dimen/sp_14"
                   />
                <ImageView
                    android:id="@+id/iv_car_photo"
                    android:layout_width="@dimen/dp_200"
                    android:layout_height="@dimen/dp_138"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_10"
                    android:src="@drawable/car_example" />

                <Button
                    android:id="@+id/btn_car_photo"
                    android:layout_width="@dimen/dp_200"
                    android:layout_height="@dimen/dp_40"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_14"
                    android:background="@drawable/shape_upload_btn"
                    android:gravity="center"
                    android:text="@string/upload_pic"
                    android:textColor="@color/color_5D97C6"
                    android:textSize="@dimen/sp_16" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />
                <TextView
                    android:id="@+id/tv3822"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/car_paper_photo"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginTop="@dimen/dp_16"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/sp_15"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv3812"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/driving_licence_hint"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginTop="@dimen/dp_10"
                    android:textColor="@color/ysf_grey_999999"
                    android:textSize="@dimen/sp_14"
                    />
                <ImageView
                    android:id="@+id/iv_driving_licence"
                    android:layout_width="@dimen/dp_200"
                    android:layout_height="@dimen/dp_138"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_10"
                    android:src="@drawable/driver_example" />

                <Button
                    android:id="@+id/btn_driving_licence"
                    android:layout_width="@dimen/dp_200"
                    android:layout_height="@dimen/dp_40"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_14"
                    android:background="@drawable/shape_upload_btn"
                    android:gravity="center"
                    android:text="@string/upload_pic"
                    android:textColor="@color/color_5D97C6"
                    android:textSize="@dimen/sp_16" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@color/ysf_grey_e9e9e9" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_17"
                    android:layout_marginTop="@dimen/dp_10"
                    android:layout_marginRight="@dimen/dp_17"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/cb_licences"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:button="@drawable/selector_cb_register_licence"
                        android:paddingLeft="@dimen/dp_8"
                        android:text="@string/hint25"
                        android:textColor="@color/textcolor"
                        android:textSize="@dimen/sp_13" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/licence_a"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/sp_13" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/licence_b"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/sp_13" />

                </LinearLayout>

                <Button
                    android:id="@+id/btn_submit"
                    android:layout_width="@dimen/dp_150"
                    android:layout_height="@dimen/dp_40"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/dp_30"
                    android:layout_marginBottom="@dimen/dp_30"
                    android:background="@drawable/shape_circle_conner_blue_deep"
                    android:gravity="center"
                    android:text="@string/submit"
                    android:textColor="@color/white"
                    android:textSize="@dimen/sp_18" />


            </LinearLayout>
        </ScrollView>

        <android.inputmethodservice.KeyboardView
            android:id="@+id/keyboard_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_gravity="bottom"
            android:background="#DCDCDC"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:keyBackground="@drawable/selector_key"
            android:keyTextColor="#000"
            android:keyTextSize="18sp"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"
            android:shadowColor="#FFFFFF"
            android:shadowRadius="0.0"
            android:visibility="gone" />
    </FrameLayout>
</LinearLayout>